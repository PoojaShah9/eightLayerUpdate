  "version": "0.0.9.1",
  "license": "MIT",
  "main": "main.js",
  "postinstall": "electron-builder install-app-deps",
  "author": "Hyden",
  "build": {
    "appId": "com.hyden.autoupdate",
    "productName": "EightLayer",
    "win": {
      "icon": "icons/icon.ico",
      "target": "NSIS"
